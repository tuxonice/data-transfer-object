{
    "name": "tuxonice/transfer-objects",
    "description": "Build data transfer objects from json definition",
    "keywords" : [
        "dto",
        "data-transfer-objects"
    ],
    "license" : "MIT",
    "authors": [
        {
            "name": "Helder Correia",
            "email": "tuxboy@tlab.pt",
            "role" : "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tlab\\TransferObjects\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {

    },
    "require-dev": {
        "symfony/var-dumper": "6.3.8",
        "squizlabs/php_codesniffer": "3.7.2",
        "phpstan/phpstan": "1.10.43",
        "phpunit/phpunit": "10.4.2"
    },
    "minimum-stability" : "stable"
}
